<%# partial view %>

    <div class="form-group">
        <label for="title">Title</label>
        <input required type="text" name="title" id="title" value="<%=article.title %>" class=" form-control">
    </div>
    <br>
    <div class="form-group">
        <label for="description">Description</label>
        <textarea name="description" id="description" class="form-control"><%= article.description %></textarea>
    </div>
    <br>
    <div class="form-group">
        <label for="markdown">Markdown</label>
        <textarea name="markdown" id="markdown" class="form-control"><%= article.markdown %></textarea>
    </div>
    <br>
    <br>
    <a href="/" class="btn btn-secondary">Cancel</a>
    <button type="submit" class="btn btn-primary ms-1">Save</button>